{
    "info": {
        "name": "drawio",
        "description": "Space-efficient and color-blind friendly theme for printing on paper with the draw.io color palette"
    },
    "params": {
        "axes": {
            "bottom": false,
            "top": false,
            "left": false,
            "right": false,
            "xmargin": 0.05,
            "ymargin": 0.02
        },
        "axis_labels": {
            "pad": -1,
            "size": "large"
        },
        "ticks": {
            "direction": "in",
            "draw_minor": false,
            "width_major": 2,
            "width_minor": 1,
            "size_major": 10,
            "size_minor": 6
        },
        "tick_labels": {
            "size": "large"
        },
        "title": {
            "location": "center",
            "pad": -2,
            "size": "x-large"
        },
        "grid": {
            "draw": true,
            "axis": "y",
            "ticks": "major",
            "alpha": 0.5,
            "width": 0.5
        },
        "fonts": {
            "family": "STIXGeneral",
            "sans-serif": [
                "Arial"
            ],
            "size": 11
        },
        "colors": {
            "figure_background_color": "white",
            "plot_background_color": "white",
            "text_color": "black",
            "line_color": "#333",
            "axes_color": "#555",
            "grid_color": "#555",
            "tick_color": "#555",
            "tick_label_color": "black",
            "palette": [
                "#FFF2CC",
                "#F8CECC",
                "#E1D5E7",
                "#D5E8D4",
                "#FFE6CC"
            ]
        }
    },
    "overrides": {},
    "transforms": {
        "trim": {
            "axis": "both"
        },
        "offset": {
            "distance": 10
        }
    }
}